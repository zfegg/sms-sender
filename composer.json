{
    "name": "zfegg/sms-sender",
    "description": "SMS sender service. And use for send sms captcha handler / \u77ed\u4fe1\u53d1\u9001\u6a21\u5757, \u5e76\u4e14\u7528\u4e8e\u53d1\u9001\u77ed\u4fe1\u9a8c\u8bc1\u7801\u5904\u7406\u7a0b\u5e8f",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "sms",
        "sender",
        "sms-sender",
        "zfegg",
        "captcha",
        "handler"
    ],
    "require": {
        "php": "^7.2 | ^8.0",
        "psr/simple-cache": "^1.0 | ^2.0 | ^3.0",
        "psr/log": "^1.0 | ^2.0 | ^3.0",
        "psr/event-dispatcher": "^1.0"
    },
    "require-dev": {
        "cache/array-adapter": "^1.0",
        "psr/http-server-handler": "^1.0",
        "phpunit/phpunit": "^7.0",
        "squizlabs/php_codesniffer": "^2.5",
        "laminas/laminas-diactoros": "^2.1",
        "laminas/laminas-inputfilter": "^2.7",
        "laminas/laminas-i18n": "^2.7",
        "laminas/laminas-captcha": "^2.5",
        "laminas/laminas-serializer": "^2.8",
        "laminas/laminas-servicemanager": "^3.0",
        "mezzio/mezzio-problem-details": "^1.0"
    },
    "authors": [
        {
            "name": "moln",
            "email": "moln.xie@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Zfegg\\SmsSender\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZfeggTest\\SmsSender\\": "test/"
        }
    },
    "extra": {
        "zf": {
            "config-provider": "Zfegg\\SmsSender\\ConfigProvider"
        }
    }
}
